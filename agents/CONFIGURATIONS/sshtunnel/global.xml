<?xml version="1.0" ?>
<set type="global">
<attribute name="agenttype" category="generic" value="sshtunnel"/>
<attribute name="homedir" value="/home/horus/horus/horus_home/sshtunnel"/>
<attribute name="emergency_homedir" value="/home/horus/horus_home/sshtunnel"/>
<attribute name="cmd" category="generic" value='/home/horus/bin/autossh -2 -fN  -M 0 -o "ServerAliveInterval 30" -o "ServerAliveCountMax 3" -R $RPORT:localhost:$LPORT $USER@$HOST'/>
<attribute name="haltAutossh" category="generic" value="killall -9 -q /home/horus/bin/autossh" />
<attribute name="haltSsh" category="generic" value="killall -9 -q /usr/bin/ssh" />
</set>
