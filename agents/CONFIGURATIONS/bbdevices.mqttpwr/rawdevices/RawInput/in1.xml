<?xml version="1.0"?>
<set type="in1">
<attribute name="type" value="booleanin"/>
<attribute name="capename" value="IN1"/>
<attribute name="gpioname" value="gpio47"/>
<attribute name="gpiopath" value="/sys/class/gpio/gpio47"/>
<attribute name="waitfor" value="0"/>
<attribute category="script" name="cmd1" value="echo 47 > /sys/class/gpio/export"/>
<attribute category="script" name="cmd2" value="echo in > /sys/class/gpio/gpio47/direction"/>
<attribute category="script" name="cmd3" value="echo both > /sys/class/gpio/gpio47/edge"/>
</set>
